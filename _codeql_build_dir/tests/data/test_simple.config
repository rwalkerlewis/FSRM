# Test Configuration for Simple Single Phase Flow Test
# This is used by test_integration.cpp

# Physics
physics_type = FLUID_FLOW

# Grid dimensions
grid_nx = 10
grid_ny = 10
grid_nz = 5

# Domain size (meters)
domain_size_x = 100.0
domain_size_y = 100.0
domain_size_z = 50.0

# Rock properties
porosity = 0.2
permeability = 100e-15  # 100 millidarcy
compressibility = 1e-9  # 1/Pa

# Fluid properties
viscosity = 1e-3    # 1 cP (water)
density = 1000.0    # kg/m^3

# Initial conditions
initial_pressure = 10e6  # 10 MPa

# Boundary conditions
bc_type = DIRICHLET
bc_left_pressure = 11e6
bc_right_pressure = 9e6

# Time stepping
timesteps = 10
dt = 86400.0  # 1 day in seconds
output_frequency = 5

# Solver options
solver_type = GMRES
preconditioner = BJACOBI
relative_tolerance = 1e-6
absolute_tolerance = 1e-10
max_iterations = 1000
