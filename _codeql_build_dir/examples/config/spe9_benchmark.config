# SPE9 Benchmark Configuration
# North Sea Reservoir Model

[SIMULATION]
name = SPE9_NorthSea
start_time = 0.0
end_time = 9.0e8                  # ~28.5 years
dt_initial = 1.0e5                # ~1 day
dt_min = 1.0e3
dt_max = 1.0e6                    # ~12 days
max_time_steps = 5000
output_frequency = 50
output_format = HDF5
output_dir = output/spe9

# Physics models
fluid_model = BLACK_OIL
enable_geomechanics = false
enable_thermal = false

# Solver settings
rtol = 1.0e-5
atol = 1.0e-7
max_iterations = 30

[GRID]
type = CARTESIAN
nx = 24
ny = 25
nz = 15
Lx = 2286.0                       # 7500 ft
Ly = 2743.2                       # 9000 ft
Lz = 213.36                       # 700 ft

[ROCK]
# Heterogeneous properties - using layer-dependent values
# Layer 1-5: High perm sand
# Layer 6-10: Medium perm sand
# Layer 11-15: Low perm shaly sand

porosity = 0.087, 0.097, 0.111, 0.160, 0.130, 0.170, 0.170, 0.080, 0.140, 0.130, 0.120, 0.105, 0.120, 0.116, 0.157

permeability_x = 20.0, 100.0, 50.0, 200.0, 300.0, 94.0, 50.0, 10.0, 30.0, 35.0, 37.0, 40.0, 30.0, 25.0, 45.0

permeability_y = 20.0, 100.0, 50.0, 200.0, 300.0, 94.0, 50.0, 10.0, 30.0, 35.0, 37.0, 40.0, 30.0, 25.0, 45.0

# Vertical permeability (typically 10% of horizontal)
permeability_z = 2.0, 10.0, 5.0, 20.0, 30.0, 9.4, 5.0, 1.0, 3.0, 3.5, 3.7, 4.0, 3.0, 2.5, 4.5

compressibility = 4.35e-10
density = 2650.0
youngs_modulus = 10.0e9
poisson_ratio = 0.25

[FLUID]
# Black oil model
oil_density = 800.0               # kg/m³
oil_viscosity = 0.001             # Pa·s (1 cP)
oil_compressibility = 1.45e-9     # 1/Pa

gas_density = 0.9                 # kg/m³ at std conditions
gas_viscosity = 0.00001           # Pa·s (0.01 cP)
gas_compressibility = 1.0e-6      # 1/Pa

water_density = 1000.0
water_viscosity = 0.0005          # Pa·s (0.5 cP)
water_compressibility = 4.35e-10

solution_GOR = 50.0               # scf/stb
bubble_point = 2.07e7             # 3000 psi

[PRESSURE]
initial_pressure = 4.14e7         # 6000 psi
pressure_gradient_z = 10350.0     # Pa/m (0.45 psi/ft)

[SATURATION]
initial_oil_saturation = 0.88
initial_water_saturation = 0.12
residual_oil_saturation = 0.20
residual_water_saturation = 0.20

[WELL1]
name = PROD1
type = PRODUCER
i = 5
j = 5
k = 1
kmax = 15
diameter = 0.1524                 # 6 inches
control_mode = BHP
target_bhp = 2.76e7               # 4000 psi
max_rate = 0.05                   # m³/s

[WELL2]
name = PROD2
type = PRODUCER
i = 19
j = 20
k = 1
kmax = 15
diameter = 0.1524
control_mode = BHP
target_bhp = 2.76e7
max_rate = 0.05

[WELL3]
name = INJ1
type = INJECTOR
i = 12
j = 12
k = 1
kmax = 15
diameter = 0.1524
fluid_type = WATER
control_mode = RATE
target_rate = 0.03                # m³/s
max_bhp = 6.89e7                  # 10000 psi

[BC1]
type = NEUMANN
field = PRESSURE
location = ALL
value = 0.0                       # No-flow boundaries

[OUTPUT]
save_pressure = true
save_saturation = true
save_well_data = true
save_performance_data = true
save_subsurface_properties = true
