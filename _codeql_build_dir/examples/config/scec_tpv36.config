# =============================================================================
# SCEC TPV36: Bimaterial Interface - Slip-Weakening
# =============================================================================
# Reference: https://strike.scec.org/cvws/tpv36_37docs.html
#
# Fault at interface between two materials with different elastic properties.
# Tests asymmetric rupture behavior due to material contrast.
# =============================================================================

[SIMULATION]
name = scec_tpv36
description = SCEC TPV36 - Bimaterial Interface
dimension = 3
physics = ELASTODYNAMICS
time_integration = EXPLICIT

start_time = 0.0
end_time = 12.0
dt = 0.002
output_frequency = 50
output_dir = output/scec_tpv36
output_format = HDF5

[GRID]
nx = 301
ny = 151
nz = 151
Lx = 30000.0
Ly = 15000.0
Lz = 15000.0
dx = 100.0
dy = 100.0
dz = 100.0

# Bimaterial structure
[MATERIAL]
type = BIMATERIAL

# Material 1 (y < 0, "faster" side)
[MATERIAL1]
region = y_negative
density = 2670.0
vp = 6000.0
vs = 3464.0

# Material 2 (y > 0, "slower" side)
[MATERIAL2]
region = y_positive
density = 2500.0
vp = 4800.0
vs = 2771.0

[FAULT1]
name = bimaterial_fault
type = STRIKE_SLIP
geometry = PLANAR
strike = 0.0
dip = 90.0
length = 30000.0
width = 15000.0
center = 15000.0, 0.0, 7500.0

friction_model = SLIP_WEAKENING
static_friction = 0.677
dynamic_friction = 0.525
Dc = 0.40

normal_stress = -120.0e6
shear_stress_along_strike = 70.0e6

nucleation_type = OVERSTRESSED_PATCH
nucleation_center = 15000.0, 0.0, 7500.0
nucleation_radius = 1500.0
nucleation_shear_stress = 81.6e6

[BC1]
type = FREE_SURFACE
location = ZMIN

[BC2]
type = ABSORBING
location = XMIN

[BC3]
type = ABSORBING
location = XMAX

[BC4]
type = ABSORBING
location = YMIN

[BC5]
type = ABSORBING
location = YMAX

[BC6]
type = ABSORBING
location = ZMAX

# Receivers on both sides
[RECEIVER1]
type = FAULT
location = 0.0, 0.0, 7500.0
output_fields = slip, slip_rate, shear_stress

[RECEIVER2]
type = FAULT
location = 7500.0, 0.0, 7500.0
output_fields = slip, slip_rate, shear_stress

[RECEIVER3]
type = FAULT
location = 22500.0, 0.0, 7500.0
output_fields = slip, slip_rate, shear_stress

# Body receivers on fast side
[RECEIVER4]
type = BODY
location = 15000.0, -3000.0, 0.0
output_fields = velocity, displacement

# Body receivers on slow side
[RECEIVER5]
type = BODY
location = 15000.0, 3000.0, 0.0
output_fields = velocity, displacement

[SOLVER]
method = EXPLICIT
cfl = 0.5
