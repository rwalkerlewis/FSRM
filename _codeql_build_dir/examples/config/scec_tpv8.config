# =============================================================================
# SCEC TPV8: 3D Strike-Slip with Rate-Dependent Friction
# =============================================================================
# Reference: https://strike.scec.org/cvws/tpv8docs.html
#
# Rate-dependent friction law with steady-state velocity dependence.
# Tests velocity-weakening vs velocity-strengthening behavior.
# =============================================================================

[SIMULATION]
name = scec_tpv8
description = SCEC TPV8 - Rate-Dependent Friction
dimension = 3
physics = ELASTODYNAMICS
time_integration = EXPLICIT

start_time = 0.0
end_time = 12.0
dt = 0.001
output_frequency = 100
output_dir = output/scec_tpv8
output_format = HDF5

[GRID]
nx = 301
ny = 151
nz = 151
Lx = 30000.0
Ly = 15000.0
Lz = 15000.0
dx = 100.0
dy = 100.0
dz = 100.0

[MATERIAL]
type = ELASTIC_HOMOGENEOUS
density = 2670.0
vp = 6000.0
vs = 3464.0

[FAULT1]
name = main_fault
type = STRIKE_SLIP
geometry = PLANAR
strike = 0.0
dip = 90.0
length = 30000.0
width = 15000.0
center = 15000.0, 0.0, 7500.0

# Rate-dependent friction
friction_model = RATE_DEPENDENT
reference_friction = 0.6
reference_velocity = 1.0e-6
direct_effect_a = 0.008
evolution_effect_b = 0.012
state_evolution_distance = 0.02

# Velocity-weakening zone
[VW_ZONE]
region = RECTANGULAR
x_min = 0.0
x_max = 22500.0
z_min = 0.0
z_max = 15000.0
a = 0.008
b = 0.012

# Velocity-strengthening barrier
[VS_ZONE]
region = RECTANGULAR
x_min = 22500.0
x_max = 30000.0
z_min = 0.0
z_max = 15000.0
a = 0.016
b = 0.008

# Initial stress
normal_stress = -120.0e6
shear_stress_along_strike = 75.0e6

# Nucleation
nucleation_type = TIME_WEAKENING
nucleation_center = 15000.0, 0.0, 7500.0
nucleation_radius = 3000.0
nucleation_time = 0.5

[BC1]
type = FREE_SURFACE
location = ZMIN

[BC2]
type = ABSORBING
location = XMIN

[BC3]
type = ABSORBING
location = XMAX

[BC4]
type = ABSORBING
location = YMIN

[BC5]
type = ABSORBING
location = YMAX

[BC6]
type = ABSORBING
location = ZMAX

[RECEIVER1]
type = FAULT
location = 0.0, 0.0, 7500.0
output_fields = slip, slip_rate, shear_stress, state_variable

[RECEIVER2]
type = FAULT
location = 4500.0, 0.0, 7500.0
output_fields = slip, slip_rate, shear_stress, state_variable

[RECEIVER3]
type = FAULT
location = 9000.0, 0.0, 7500.0
output_fields = slip, slip_rate, shear_stress, state_variable

[SOLVER]
method = EXPLICIT
cfl = 0.5
