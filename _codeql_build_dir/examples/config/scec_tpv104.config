# =============================================================================
# SCEC TPV104: Inelastic Benchmark - Viscoplasticity
# =============================================================================
# Reference: https://strike.scec.org/cvws/tpv104docs.html
#
# Rate-dependent (viscoplastic) off-fault deformation.
# Tests time-dependent plastic flow in the damage zone.
# =============================================================================

[SIMULATION]
name = scec_tpv104
description = SCEC TPV104 - Viscoplasticity
dimension = 3
physics = ELASTODYNAMICS_VISCOPLASTIC
time_integration = EXPLICIT

start_time = 0.0
end_time = 15.0
dt = 0.001
output_frequency = 100
output_dir = output/scec_tpv104
output_format = HDF5

[GRID]
nx = 301
ny = 151
nz = 151
Lx = 30000.0
Ly = 15000.0
Lz = 15000.0
dx = 100.0
dy = 100.0
dz = 100.0

[MATERIAL]
type = VISCOPLASTIC
density = 2670.0
vp = 6000.0
vs = 3464.0

# Viscoplastic parameters
plasticity_model = PERZYNA
cohesion = 5.0e6
friction_angle = 30.0
fluidity = 1.0e-3
rate_sensitivity = 0.1

# Relaxation time scale
relaxation_time = 0.01

[FAULT1]
name = main_fault
type = STRIKE_SLIP
geometry = PLANAR
strike = 0.0
dip = 90.0
length = 30000.0
width = 15000.0
center = 15000.0, 0.0, 7500.0

friction_model = SLIP_WEAKENING
static_friction = 0.677
dynamic_friction = 0.525
Dc = 0.40

normal_stress = -120.0e6
shear_stress_along_strike = 70.0e6

nucleation_type = OVERSTRESSED_PATCH
nucleation_center = 15000.0, 0.0, 7500.0
nucleation_radius = 1500.0
nucleation_shear_stress = 81.6e6

[BC1]
type = FREE_SURFACE
location = ZMIN

[BC2]
type = ABSORBING
location = XMIN

[BC3]
type = ABSORBING
location = XMAX

[BC4]
type = ABSORBING
location = YMIN

[BC5]
type = ABSORBING
location = YMAX

[BC6]
type = ABSORBING
location = ZMAX

[RECEIVER1]
type = FAULT
location = 0.0, 0.0, 7500.0
output_fields = slip, slip_rate, shear_stress

[RECEIVER2]
type = FAULT
location = 7500.0, 0.0, 7500.0
output_fields = slip, slip_rate, shear_stress

[RECEIVER3]
type = BODY
location = 0.0, 500.0, 7500.0
output_fields = velocity, viscoplastic_strain, strain_rate

[RECEIVER4]
type = BODY
location = 0.0, 1000.0, 7500.0
output_fields = velocity, viscoplastic_strain, strain_rate

[SOLVER]
method = EXPLICIT
cfl = 0.5
