# =============================================================================
# SCEC TPV7: 3D Strike-Slip with Velocity-Strengthening Barrier
# =============================================================================
# Reference: https://strike.scec.org/cvws/tpv7docs.html
#
# Similar to TPV6 but with a velocity-strengthening barrier zone that
# arrests rupture propagation.
# =============================================================================

[SIMULATION]
name = scec_tpv7
description = SCEC TPV7 - Strike-Slip with VS Barrier
dimension = 3
physics = ELASTODYNAMICS
time_integration = EXPLICIT

start_time = 0.0
end_time = 12.0
dt = 0.002
output_frequency = 50
output_dir = output/scec_tpv7
output_format = HDF5

[GRID]
nx = 301
ny = 151
nz = 151
Lx = 30000.0
Ly = 15000.0
Lz = 15000.0
dx = 100.0
dy = 100.0
dz = 100.0

[MATERIAL]
type = ELASTIC_HOMOGENEOUS
density = 2670.0
vp = 6000.0
vs = 3464.0

[FAULT1]
name = main_fault
type = STRIKE_SLIP
geometry = PLANAR
strike = 0.0
dip = 90.0
length = 30000.0
width = 15000.0
center = 15000.0, 0.0, 7500.0

# Mixed friction zones
friction_model = SLIP_WEAKENING_WITH_BARRIER

# Main fault (velocity-weakening)
[FRICTION_ZONE1]
name = vw_zone
region = RECTANGULAR
x_min = 0.0
x_max = 22500.0
z_min = 0.0
z_max = 15000.0
static_friction = 0.677
dynamic_friction = 0.525
Dc = 0.40

# Barrier zone (velocity-strengthening - higher friction)
[FRICTION_ZONE2]
name = vs_barrier
region = RECTANGULAR
x_min = 22500.0
x_max = 30000.0
z_min = 0.0
z_max = 15000.0
static_friction = 1.0e6
dynamic_friction = 1.0e6
Dc = 0.40

# Initial stress
normal_stress = -120.0e6
shear_stress_along_strike = 70.0e6

# Nucleation
nucleation_type = OVERSTRESSED_PATCH
nucleation_center = 15000.0, 0.0, 7500.0
nucleation_radius = 1500.0
nucleation_shear_stress = 81.6e6

[BC1]
type = FREE_SURFACE
location = ZMIN

[BC2]
type = ABSORBING
location = XMIN

[BC3]
type = ABSORBING
location = XMAX

[BC4]
type = ABSORBING
location = YMIN

[BC5]
type = ABSORBING
location = YMAX

[BC6]
type = ABSORBING
location = ZMAX

[RECEIVER1]
type = FAULT
location = 0.0, 0.0, 7500.0
output_fields = slip, slip_rate, shear_stress

[RECEIVER2]
type = FAULT
location = 7500.0, 0.0, 7500.0
output_fields = slip, slip_rate, shear_stress

[RECEIVER3]
type = FAULT
location = 15000.0, 0.0, 7500.0
output_fields = slip, slip_rate, shear_stress

[RECEIVER4]
type = FAULT
location = 22500.0, 0.0, 7500.0
output_fields = slip, slip_rate, shear_stress

[SOLVER]
method = EXPLICIT
cfl = 0.5
