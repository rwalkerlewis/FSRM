# Simple Elastostatics Test
# Tests static deformation under gravity and boundary loads

[SIMULATION]
name = elastostatics_test
start_time = 0.0
end_time = 1.0                      # Single timestep (quasi-static)
dt_initial = 1.0
dt_min = 1.0
dt_max = 1.0
output_frequency = 1
output_format = HDF5
fluid_model = SINGLE_COMPONENT
solid_model = ELASTIC
enable_geomechanics = true          # Enable elasticity
enable_thermal = false
enable_fractures = false
enable_faults = false
rtol = 1.0e-8
atol = 1.0e-10
max_nonlinear_iterations = 20

[GRID]
nx = 10
ny = 10
nz = 10
Lx = 100.0                          # 100 m cube
Ly = 100.0
Lz = 100.0

[ROCK]
# Elastic material properties
density = 2650.0                    # kg/m³ (typical rock)
youngs_modulus = 10.0e9             # 10 GPa (Pa)
poissons_ratio = 0.25               # dimensionless
porosity = 0.15                     # 15%
permeability_x = 10.0               # mD
permeability_y = 10.0
permeability_z = 1.0

[FLUID]
type = SINGLE_PHASE
density = 1000.0                    # kg/m³
viscosity = 0.001                   # Pa·s
compressibility = 4.5e-10           # 1/Pa
reference_pressure = 10.0e6         # 10 MPa

[BOUNDARY_CONDITIONS]
# Fixed bottom boundary (z=0)
bc_z_min_type = DIRICHLET
bc_z_min_displacement = 0.0

# Traction on top boundary (z=Lz)
bc_z_max_type = NEUMANN
bc_z_max_traction = -1.0e6          # -1 MPa (compression)

# Free lateral boundaries
bc_x_min_type = FREE
bc_x_max_type = FREE
bc_y_min_type = FREE
bc_y_max_type = FREE

[INITIAL_CONDITIONS]
initial_pressure = 10.0e6           # 10 MPa
initial_temperature = 300.0         # 300 K
initial_stress_xx = 0.0
initial_stress_yy = 0.0
initial_stress_zz = 0.0
