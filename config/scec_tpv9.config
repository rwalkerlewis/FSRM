# =============================================================================
# SCEC TPV9: 3D Strike-Slip with Depth-Dependent Stress
# =============================================================================
# Reference: https://strike.scec.org/cvws/tpv9docs.html
#
# Depth-dependent initial stress field following lithostatic gradient.
# Tests rupture behavior under realistic stress conditions.
# =============================================================================

[SIMULATION]
name = scec_tpv9
description = SCEC TPV9 - Depth-Dependent Stress
dimension = 3
physics = ELASTODYNAMICS
time_integration = EXPLICIT

start_time = 0.0
end_time = 12.0
dt = 0.002
output_frequency = 50
output_dir = output/scec_tpv9
output_format = HDF5

[GRID]
nx = 301
ny = 151
nz = 151
Lx = 30000.0
Ly = 15000.0
Lz = 15000.0
dx = 100.0
dy = 100.0
dz = 100.0

[MATERIAL]
type = ELASTIC_HOMOGENEOUS
density = 2670.0
vp = 6000.0
vs = 3464.0

[FAULT1]
name = main_fault
type = STRIKE_SLIP
geometry = PLANAR
strike = 0.0
dip = 90.0
length = 30000.0
width = 15000.0
center = 15000.0, 0.0, 7500.0

# Friction law
friction_model = SLIP_WEAKENING
static_friction = 0.760
dynamic_friction = 0.448
Dc = 0.50

# Depth-dependent stress
stress_model = DEPTH_DEPENDENT
surface_normal_stress = 0.0
normal_stress_gradient = -18000.0
surface_shear_stress = 0.0
shear_stress_gradient = 13600.0
max_depth = 15000.0

# Nucleation at depth
nucleation_type = OVERSTRESSED_PATCH
nucleation_center = 15000.0, 0.0, 12000.0
nucleation_radius = 1500.0
nucleation_stress_perturbation = 11.6e6

[BC1]
type = FREE_SURFACE
location = ZMIN

[BC2]
type = ABSORBING
location = XMIN

[BC3]
type = ABSORBING
location = XMAX

[BC4]
type = ABSORBING
location = YMIN

[BC5]
type = ABSORBING
location = YMAX

[BC6]
type = ABSORBING
location = ZMAX

# Receivers at different depths
[RECEIVER1]
type = FAULT
location = 0.0, 0.0, 3000.0
output_fields = slip, slip_rate, shear_stress

[RECEIVER2]
type = FAULT
location = 0.0, 0.0, 7500.0
output_fields = slip, slip_rate, shear_stress

[RECEIVER3]
type = FAULT
location = 0.0, 0.0, 12000.0
output_fields = slip, slip_rate, shear_stress

[RECEIVER4]
type = FAULT
location = 6000.0, 0.0, 7500.0
output_fields = slip, slip_rate, shear_stress

[RECEIVER5]
type = FAULT
location = -6000.0, 0.0, 7500.0
output_fields = slip, slip_rate, shear_stress

[SOLVER]
method = EXPLICIT
cfl = 0.5
