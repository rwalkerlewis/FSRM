# =============================================================================
# SCEC TPV6: 3D Strike-Slip Bilateral Rupture
# =============================================================================
# Reference: https://strike.scec.org/cvws/tpv6docs.html
#
# 3D vertical strike-slip fault with bilateral rupture propagation.
# Linear slip-weakening friction. Free surface at top.
# =============================================================================

[SIMULATION]
name = scec_tpv6
description = SCEC TPV6 - 3D Bilateral Strike-Slip
dimension = 3
physics = ELASTODYNAMICS
time_integration = EXPLICIT

start_time = 0.0
end_time = 12.0
dt = 0.002
output_frequency = 50
output_dir = output/scec_tpv6
output_format = HDF5

[GRID]
nx = 301
ny = 151
nz = 151
Lx = 30000.0
Ly = 15000.0
Lz = 15000.0
dx = 100.0
dy = 100.0
dz = 100.0

[MATERIAL]
type = ELASTIC_HOMOGENEOUS
density = 2670.0
vp = 6000.0
vs = 3464.0
youngs_modulus = 32.04e9
poissons_ratio = 0.25

[FAULT1]
name = main_fault
type = STRIKE_SLIP
geometry = PLANAR
strike = 0.0
dip = 90.0
length = 30000.0
width = 15000.0
center = 15000.0, 0.0, 7500.0

# Friction law
friction_model = SLIP_WEAKENING
static_friction = 0.677
dynamic_friction = 0.525
Dc = 0.40

# Initial stress - bilateral setup
normal_stress = -120.0e6
shear_stress_along_strike = 70.0e6
shear_stress_along_dip = 0.0

# Nucleation zone (centered)
nucleation_type = OVERSTRESSED_PATCH
nucleation_center = 15000.0, 0.0, 7500.0
nucleation_radius = 1500.0
nucleation_shear_stress = 81.6e6

[BC1]
type = FREE_SURFACE
location = ZMIN

[BC2]
type = ABSORBING
location = XMIN

[BC3]
type = ABSORBING
location = XMAX

[BC4]
type = ABSORBING
location = YMIN

[BC5]
type = ABSORBING
location = YMAX

[BC6]
type = ABSORBING
location = ZMAX

# On-fault receivers
[RECEIVER1]
type = FAULT
location = 0.0, 0.0, 7500.0
output_fields = slip, slip_rate, shear_stress

[RECEIVER2]
type = FAULT
location = 3000.0, 0.0, 7500.0
output_fields = slip, slip_rate, shear_stress

[RECEIVER3]
type = FAULT
location = -3000.0, 0.0, 7500.0
output_fields = slip, slip_rate, shear_stress

[RECEIVER4]
type = FAULT
location = 6000.0, 0.0, 7500.0
output_fields = slip, slip_rate, shear_stress

[RECEIVER5]
type = FAULT
location = -6000.0, 0.0, 7500.0
output_fields = slip, slip_rate, shear_stress

# Off-fault receivers
[RECEIVER6]
type = BODY
location = 0.0, 3000.0, 0.0
output_fields = velocity, displacement

[RECEIVER7]
type = BODY
location = 0.0, -3000.0, 0.0
output_fields = velocity, displacement

[SOLVER]
method = EXPLICIT
cfl = 0.5
