# =============================================================================
# SCEC TPV34: Off-Fault Plasticity - Homogeneous Stress
# =============================================================================
# Reference: https://strike.scec.org/cvws/tpv34_35docs.html
#
# Dynamic rupture with off-fault Drucker-Prager plasticity.
# Tests inelastic deformation in the damage zone around the fault.
# =============================================================================

[SIMULATION]
name = scec_tpv34
description = SCEC TPV34 - Off-Fault Plasticity
dimension = 3
physics = ELASTODYNAMICS_PLASTIC
time_integration = EXPLICIT

start_time = 0.0
end_time = 12.0
dt = 0.001
output_frequency = 100
output_dir = output/scec_tpv34
output_format = HDF5

[GRID]
nx = 301
ny = 151
nz = 151
Lx = 30000.0
Ly = 15000.0
Lz = 15000.0
dx = 100.0
dy = 100.0
dz = 100.0

[MATERIAL]
type = ELASTIC_PLASTIC
density = 2670.0
vp = 6000.0
vs = 3464.0

# Drucker-Prager plasticity
plasticity_model = DRUCKER_PRAGER
cohesion = 5.0e6
friction_angle = 30.0
dilation_angle = 0.0
plastic_viscosity = 0.1

# Plasticity zone near fault
plasticity_zone_width = 3000.0

[FAULT1]
name = main_fault
type = STRIKE_SLIP
geometry = PLANAR
strike = 0.0
dip = 90.0
length = 30000.0
width = 15000.0
center = 15000.0, 0.0, 7500.0

friction_model = SLIP_WEAKENING
static_friction = 0.677
dynamic_friction = 0.525
Dc = 0.40

# Homogeneous stress
normal_stress = -120.0e6
shear_stress_along_strike = 70.0e6

nucleation_type = OVERSTRESSED_PATCH
nucleation_center = 15000.0, 0.0, 7500.0
nucleation_radius = 1500.0
nucleation_shear_stress = 81.6e6

[BC1]
type = FREE_SURFACE
location = ZMIN

[BC2]
type = ABSORBING
location = XMIN

[BC3]
type = ABSORBING
location = XMAX

[BC4]
type = ABSORBING
location = YMIN

[BC5]
type = ABSORBING
location = YMAX

[BC6]
type = ABSORBING
location = ZMAX

[RECEIVER1]
type = FAULT
location = 0.0, 0.0, 7500.0
output_fields = slip, slip_rate, shear_stress

[RECEIVER2]
type = FAULT
location = 6000.0, 0.0, 7500.0
output_fields = slip, slip_rate, shear_stress

[RECEIVER3]
type = FAULT
location = 12000.0, 0.0, 7500.0
output_fields = slip, slip_rate, shear_stress

# Off-fault receivers for plastic strain
[RECEIVER4]
type = BODY
location = 0.0, 500.0, 7500.0
output_fields = velocity, plastic_strain

[RECEIVER5]
type = BODY
location = 0.0, 1000.0, 7500.0
output_fields = velocity, plastic_strain

[RECEIVER6]
type = BODY
location = 0.0, 2000.0, 7500.0
output_fields = velocity, plastic_strain

[SOLVER]
method = EXPLICIT
cfl = 0.5
