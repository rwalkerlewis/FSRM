# ==============================================================================
# Gmsh Binary Mesh Example (PETSc DMPlex Gmsh reader)
# ==============================================================================
# This example demonstrates loading a *binary* Gmsh .msh file.
#
# 1) Generate the mesh:
#    ./scripts/generate_binary_box_mesh.sh
#
# 2) Run:
#    mpirun -np 2 fsrm -c config/gmsh_binary_box_example.config -dm_view
# ==============================================================================

[SIMULATION]
name = gmsh_binary_box_example
start_time = 0.0
end_time = 10.0
dt_initial = 1.0
max_timesteps = 10
output_frequency = 1

fluid_model = SINGLE_COMPONENT
solid_model = ELASTIC
enable_geomechanics = false
enable_thermal = false

rtol = 1.0e-6
atol = 1.0e-8

[GRID]
mesh_type = GMSH
use_unstructured = true
mesh_file = meshes/binary_box/binary_box_3d_bin.msh

# Map the physical volume to a material section
gmsh_material_mapping = reservoir:ROCK

# Optional: label some boundary faces by physical surface names
gmsh_boundaries = xmin, xmax, ymin, ymax, zmin, zmax

[ROCK]
name = SimpleRock
porosity = 0.20
permeability_x = 100 mD
permeability_y = 100 mD
permeability_z = 100 mD
youngs_modulus = 10 GPa
poisson_ratio = 0.25
density = 2500 kg/m3

[FLUID]
type = SINGLE_PHASE
density = 1000 kg/m3
viscosity = 1 cP
compressibility = 4.5e-10 1/Pa
