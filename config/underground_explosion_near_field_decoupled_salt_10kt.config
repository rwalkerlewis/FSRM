# ==============================================================================
# Near-Field Underground Explosion (Decoupled Salt Cavity, 10 kt)
# ==============================================================================
# For `./underground_explosion_near_field <config_file>`
#
# This illustrates the same "decoupled" concept as the full-scale config:
# - salt host properties
# - lower strength and different wave speeds
# - reduced overshoot (more contained)
# ==============================================================================

[EXPLOSION_SOURCE]
yield_kt = 10.0
depth_of_burial = 800.0
source_x = 0.0
source_y = 0.0

# Coupling / source shape knob (NearFieldExplosionConfig reads this)
overshoot_factor = 1.05

[HOST_ROCK]
host_density = 2160.0
p_wave_velocity = 4500.0
s_wave_velocity = 2500.0
porosity = 0.005

[HOST_ROCK.EOS]
bulk_sound_speed = 3800.0
hugoniot_slope = 1.3
gruneisen_gamma = 1.7
gruneisen_a = 0.4

[HOST_ROCK.STRENGTH]
cohesion = 10.0e6
pressure_coefficient = 0.35
pressure_exponent = 0.5
tension_coefficient = 3.0
max_strength = 0.8e9
residual_strength = 0.05e6
softening_strain = 0.06
residual_strength_ratio = 0.12

[DAMAGE]
damage_strain_threshold = 0.001
damage_strain_saturation = 0.12
damage_pressure_threshold = 0.6e9
damage_pressure_saturation = 6.0e9
tensile_damage_threshold = 2.0e6
damage_rate = 8.0
max_damage = 0.99

